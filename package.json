{
  "name": "fuzzy-octo-guacamole",
  "version": "1.1.0",
  "description": " The exhaustive Pattern Matching library for TypeScript.",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.modern.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.cjs"
      }
    }
  },
  "main": "index.cjs",
  "types": "index.d.ts",
  "module": "index.module.js",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "index.cjs",
    "index.cjs.map",
    "index.d.ts",
    "index.modern.js",
    "index.modern.js.map",
    "index.module.js",
    "index.module.js.map",
    "index.umd.js",
    "index.umd.js.map",
    "internals/helpers.d.ts",
    "internals/symbols.d.ts",
    "is-matching.d.ts",
    "match.d.ts",
    "patterns.d.ts",
    "types/BuildMany.d.ts",
    "types/DeepExclude.d.ts",
    "types/DistributeUnions.d.ts",
    "types/ExtractPreciseValue.d.ts",
    "types/FindSelected.d.ts",
    "types/InvertPattern.d.ts",
    "types/IsMatching.d.ts",
    "types/Match.d.ts",
    "types/Pattern.d.ts",
    "types/helpers.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/garronej/fuzzy-octo-guacamole.git"
  },
  "keywords": [
    "pattern",
    "matching",
    "pattern-matching",
    "typescript",
    "match-with",
    "match",
    "switch",
    "adt"
  ],
  "author": "Gabriel Vergnaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garronej/fuzzy-octo-guacamole/issues"
  },
  "homepage": "https://github.com/garronej/fuzzy-octo-guacamole#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.4.5",
    "microbundle": "^0.15.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "denoify": "^1.2.1",
    "typescript": "^4.8.3"
  }
}
